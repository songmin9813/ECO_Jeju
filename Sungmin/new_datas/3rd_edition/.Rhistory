a <- a$Left
b <- c(span2[[1]][8])
b <- b$Joined
move1 <- c(move1,a)
team1 <- c(team1,rep(i,length(a)))
move2 <- c(move2,b)
team2 <- c(team2,rep(i,length(b)))
}
result1 <- data.frame(FROM=team1,TO=move1)
result2 <- data.frame(FROM=team2,TO=move2)
total <- rbind(result1, result2)
total <- total[order(total$FROM),]
rownames(total) <- 1:719
total

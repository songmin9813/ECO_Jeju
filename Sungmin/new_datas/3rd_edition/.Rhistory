span1 <- html_nodes(x=html, xpath=paste0('//*[@id="main"]/div[11]/div[1]/div[',as.character(i),']/div[2]/table')) %>% html_table()
span2 <- html_nodes(x=html, xpath=paste0('//*[@id="main"]/div[11]/div[1]/div[',as.character(i),']/div[4]/table')) %>% html_table()
